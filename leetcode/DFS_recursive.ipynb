   "source": [
    "graph = { 1:[2,3,4], 2:[5], 3:[5], 4:[], 5 : [6,7], 6 :[], 7:[3],}\n",
    "\n",
    "def dfs_recursive(v, discovered=[]):\n",
    "    discovered.append(v)\n",
    "    for i in graph[v]:\n",
    "        if i not in  discovered:\n",
    "            dfs_recursive(i,discovered)\n",
    "    return discovered\n",
    "\n",
    "f'discovered : {dfs_recursive(1)}'"
   ]
  }
